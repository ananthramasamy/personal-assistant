<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressView">
        <attr name="removeFromParentOnEnd" format="boolean" />
        <attr name="circleRadius" format="reference" />
        <attr name="rotationRadius" format="reference" />
        <attr name="rotationDuration" format="integer" />
        <attr name="splashDuration" format="integer" />
        <attr name="splashBackgroundColor" format="color" />
        <attr name="circleColors" format="reference" />
    </declare-styleable>
    <declare-styleable name="FloatingLabel">
        <attr name="floatHintText" format="string" />
        <attr name="floatHintTextColorFocused" format="color" />
        <attr name="floatHintTextColorUnFocused" format="color" />
        <attr name="floatHintTextStyle">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="floatHintTextGravity">
            <enum name="top" value="0x30" />
            <enum name="bottom" value="0x50" />
            <enum name="left" value="0x03" />
            <enum name="right" value="0x05" />
        </attr>
        <attr name="floatHintTextSize" format="integer" />
        <attr name="floatHintTextTypeface" format="string" />
        <attr name="floatHintTextBackground" format="color|reference" />
        <!-- For actual Text -->

        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textStyle">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="textGravity">
            <enum name="top" value="0x30" />
            <enum name="bottom" value="0x50" />
            <enum name="left" value="0x03" />
            <enum name="right" value="0x05" />
        </attr>
        <attr name="textSize" format="integer" />
        <attr name="textTypeface" format="string" />
        <attr name="textBackground" format="color|reference" />
        <attr name="isPassword" format="boolean" />
    </declare-styleable>

    <!--expandable cardview-->
    <declare-styleable name="ExpandableCardView">
        <attr name="title" format="string"/>
        <attr name="icon" format="reference"/>
        <attr name="inner_view" format="reference"/>
        <attr name="expandOnClick" format="boolean"/>
        <attr name="animationDuration" format="integer"/>
        <attr name="startExpanded" format="boolean"/>
    </declare-styleable>
    <!--end-->


    <declare-styleable name="ExpansionHeader">
        <attr name="expansion_layout" format="reference"/>
        <attr name="expansion_toggleOnClick" format="boolean"/>
        <attr name="expansion_headerIndicator" format="reference"/>
        <attr name="expansion_headerIndicatorRotationExpanded" format="integer"/>
        <attr name="expansion_headerIndicatorRotationCollapsed" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ExpansionLayout">
        <attr name="expansion_expanded" format="boolean"/>
    </declare-styleable>

    <attr name="expansion_openOnlyOne" format="boolean"/>

    <declare-styleable name="ExpansionsViewGroupFrameLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupLinearLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupRelativeLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupConstraintLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="RippleBackground">
        <attr name="rp_color" format="color" />
        <!--alpha from 0 .. 255-->
        <attr name="rp_alpha" format="integer"/>
        <attr name="rp_duration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CrystalSeekBar" >
        <attr name="corner_radius" />
        <attr name="min_value" />
        <attr name="max_value" />
        <attr name="min_start_value" />
        <attr name="max_start_value" />
        <attr name="steps" />
        <attr name="gap" />
        <attr name="fix_gap" />
        <attr name="bar_height" />
        <attr name="bar_color_mode" />
        <attr name="bar_color" />
        <attr name="bar_gradient_start" />
        <attr name="bar_gradient_end" />
        <attr name="bar_highlight_color_mode" />
        <attr name="bar_highlight_color" />
        <attr name="bar_highlight_gradient_start" />
        <attr name="bar_highlight_gradient_end" />
        <attr name="thumb_diameter" />
        <attr name="position" />
        <attr name="data_type" />
        <attr name="thumb_color" format="color" />
        <attr name="thumb_color_pressed" format="color" />
        <attr name="thumb_image" format="reference" />
        <attr name="thumb_image_pressed" format="reference" />
        <attr name="seek_bar_touch_enabled" />
    </declare-styleable>


</resources>